{
  "name": "ProfeAI Development",
  "image": "python:3.11",
  "forwardPorts": [8000],
  "postCreateCommand": "pip install -r requirements.txt",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "portsAttributes": {
    "8000": {
      "label": "ProfeAI Server",
      "onAutoForward": "openBrowser"
    }
  }
}